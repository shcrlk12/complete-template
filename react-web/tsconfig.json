{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "noEmit":false,
      "baseUrl": ".",
      "outDir": "./dist/",

      "paths": {
        "@components/*": ["src/components/*"],
        "@images/*": ["src/assets/images/*"],
        "@style/*": ["src/style/*"],
        "@pages/*": ["src/pages/*"],
        "@reducers/*": ["src/reducers/*"],
        "@src/*": ["src/*"],
      }
      },
    "include": [
      "**/*.d.ts", "**/*.ts", "**/*.tsx"
    ],
  }